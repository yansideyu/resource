@font-face {
  font-family: 'AlibabaPuHuiTi_2_55_Regular';
  src: url('data:font/ttf;charset=utf-8;base64,') format('truetype');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: 'AlibabaPuHuiTi_2_85_Bold';
  src: url('data:font/ttf;charset=utf-8;base64,') format('truetype');
  font-weight: bold;
  font-style: normal;
  font-display: swap;
}

.font-pr {
  font-family: PingFangSC-Regular;
  font-weight: 400;
}

.font-pm {
  font-family: PingFangSC-Medium;
