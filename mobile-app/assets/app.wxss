.line-2 {
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}

.line-1 {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}